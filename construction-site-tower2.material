import * from "managed_mats.material"
material construction-site-tow : RoR/Managed_Mats/Base
{
	receive_shadows on
   technique BaseTechnique
    {
        pass BaseRender
        {
            texture_unit Diffuse_Map
            {
            texture Crane-man-yellow.dds
			tex_address_mode wrap
                scale 1.0 1.0
                tex_coord_set 0
                colour_op modulate
            }
            texture_unit 
            {
                texture AO-test.png
                tex_address_mode wrap
                scale 1.0 1.0
                tex_coord_set 1
                colour_op modulate
            }
        }
    }
}

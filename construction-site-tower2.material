material construction-site-tower_doorzichtig : RoR/Managed_Mats/Base
{
   technique 
   {
      pass
      {
         scene_blend alpha_blend
         depth_check on
         depth_write off
         diffuse 1.0 1.0 1.0 0.0
         ambient 1.0 1.0 1.0 0.0
      }
   }
}

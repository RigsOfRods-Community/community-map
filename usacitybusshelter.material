material shelterglass : RoR/Managed_Mats/Base
{
	technique BaseTechnique
	{
		pass BaseRender
		{
			scene_blend alpha_blend
			depth_check on
			depth_write off
			diffuse 1 1 1 0.48
			texture_unit Diffuse_Map
			{
                texture usacityglass.png
            }
        }
    }
}

import * from "managed_mats.material"
material alt_asphalt1  : RoR/Managed_Mats/Base
{
	technique BaseTechnique
	{
		pass BaseRender
		{

			texture_unit Diffuse_Map
			{
                texture usacityasphalt1.dds

            }
        }
    }
}

